<h1>Edit puppy :(</h1>

<%= form_for @puppy, url:"/puppies" do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :age %>
<%= f.text_field :age %>
<%= f.label :breed %>
<%= f.text_field :breed %>

<%= f.submit %>

<% end %>
<p>
<%= form_for @puppy, url:"/puppies/:id" do |f| %>
<%= f.label :Delete %>
<%= f.submit %>
</p>
<% end %>



<p>
<a href="/puppies">Back to puppies!</a>
</p>